#
#  Copyright (c) 2024: Jacob.Lundqvist@gmail.com
#
#  Displays iSH version
#

ish_version="$(cat /proc/ish/version 2>/dev/null)"
[ -z "$ish_version" ] && ish_version="can't be detected whilst chrooted"
echo "iSH release:     $ish_version"
